{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "identifier": "com.golmakov.sketch-hyphenator",
  "commands": [
    {
      "name": "Hyphenate Text",
      "identifier": "hyphenate-text",
      "icon": "hyphenate.png",
      "handler": "addHyphens",
      "script": "hyphenate.js"
    },
    {
      "name": "Remove Hyphens",
      "identifier": "hyphens-remove",
      "icon": "removeHyphens.png",
      "handler": "removeHyphens",
      "script": "hyphenate.js"
    }
  ],
  "menu": {
    "title": "Hyphenator",
    "items": [
      "hyphenate-text",
      "hyphens-remove"
    ]
  },
  "version": "0.1.2",
  "description": "Sketch plugin that hyphenates text",
  "homepage": "https://github.com/golmakov/sketch-hyphenator",
  "name": "sketch-hyphenator",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com/golmakov/sketch-hyphenator/master/.appcast.xml",
  "author": "Aleksander Golmakov"
}