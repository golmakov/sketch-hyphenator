{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "identifier": "com.golmakov.sketch-hyphenator",
  "commands": [
    {
      "name": "Hyphenate Text",
      "identifier": "hyphenate-text",
      "icon" : "hyphenate.png",
      "handler": "addHyphens",
      "script": "./hyphenate.js"
    },
    {
      "name": "Remove Hyphens",
      "identifier": "hyphens-remove",
      "icon" : "removeHyphens.png",
      "handler": "removeHyphens",
      "script": "./hyphenate.js"
    }
  ],
  "menu": {
    "title": "Hyphenator",
    "items": [
      "hyphenate-text",
      "hyphens-remove"
    ]
  }
}